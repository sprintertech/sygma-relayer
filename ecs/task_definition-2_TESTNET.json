{
  "containerDefinitions": [
    {
      "name": "relayer-container-TESTNET",
      "essential": true,
      "image": "852551629426.dkr.ecr.us-east-2.amazonaws.com/relayer:stable",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/relayer-2-TESTNET",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "command": [
        "run",
        "--config=env",
        "--name=R2",
        "--blockstore=/mount/relayer2/lvldbdata"
      ],
      "portMappings": [
        {
          "containerPort": 9000,
          "hostPort": 9000,
          "protocol": "tcp"
        },
        {
          "containerPort": 9001,
          "hostPort": 9001,
          "protocol": "tcp"
        }
      ],
      "mountPoints": [
        {
          "containerPath": "/mount",
          "sourceVolume": "efs"
        }
      ],
      "environment": [
        {
          "value": "relayer-2",
          "name": "DD_SERVICE"
        },
        {
          "name": "SYG_RELAYER_LOGLEVEL",
          "value": "debug"
        }
      ],
      "secrets": [
        {
          "name": "SYG_RELAYER_MPCCONFIG_KEY",
          "valueFrom": "arn:aws:ssm:us-east-2:852551629426:parameter/sygma/testnet/relayers/relayer-2/SYG_RELAYER_MPCCONFIG_KEY"
        },
        {
          "name": "SYG_RELAYER_MPCCONFIG_KEYSHAREPATH",
          "valueFrom": "arn:aws:ssm:us-east-2:852551629426:parameter/sygma/testnet/relayers/relayer-2/SYG_RELAYER_MPCCONFIG_KEYSHAREPATH"
        },
        {
          "name": "SYG_RELAYER_MPCCONFIG_TOPOLOGYCONFIGURATION_ENCRYPTIONKEY",
          "valueFrom": "arn:aws:ssm:us-east-2:852551629426:parameter/sygma/testnet/relayers/relayer-0/SYG_RELAYER_MPCCONFIG_TOPOLOGYCONFIGURATION_ENCRYPTIONKEY"
        },
        {
          "name": "SYG_RELAYER_MPCCONFIG_TOPOLOGYCONFIGURATION_PATH",
          "valueFrom": "arn:aws:ssm:us-east-2:852551629426:parameter/sygma/testnet/relayers/relayer-2/SYG_RELAYER_MPCCONFIG_TOPOLOGYCONFIGURATION_PATH"
        },
        {
          "name": "SYG_RELAYER_MPCCONFIG_TOPOLOGYCONFIGURATION_URL",
          "valueFrom": "arn:aws:ssm:us-east-2:852551629426:parameter/sygma/testnet/relayers/relayer-0/SYG_RELAYER_MPCCONFIG_TOPOLOGYCONFIGURATION_URL"
        },
        {
          "name": "SYG_DOM_1",
          "valueFrom": "arn:aws:ssm:us-east-2:852551629426:parameter/sygma/testnet/relayers/relayer-2/SYG_DOM_1"
        },
        {
          "name": "SYG_DOM_2",
          "valueFrom": "arn:aws:ssm:us-east-2:852551629426:parameter/sygma/testnet/relayers/relayer-2/SYG_DOM_2"
        },
        {
          "name": "SYG_DOM_3",
          "valueFrom": "arn:aws:ssm:us-east-2:852551629426:parameter/sygma/testnet/relayers/relayer-2/SYG_DOM_3"
        }
      ]
    }
  ],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::852551629426:role/relayer-2-ecsTaskExecutionRole",
  "family": "relayer-container-TESTNET",
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  },
  "volumes": [
    {
      "name": "efs",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-0c20812e9a3c5dfc4",
        "transitEncryption": "ENABLED"
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ]
}
